    "aws-sdk": "^2.991.0",
    "bson": "^4.4.1",
    "crypto": "^1.0.1",
    "date-fns": "^2.25.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js-sha1": "^0.6.0",
    "moment": "^2.29.3",
    "node-cron": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.29.1",
    "socket.io": "^4.1.3",
    "tldts": "^5.7.53",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0"
