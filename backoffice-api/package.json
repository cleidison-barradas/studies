  "description": "Backoffice API",
  "main": "app.ts",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "tsc",
    "start": "node -r ts-node/register src/app.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --coverage"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.0.0"
  },
  "author": "George Carvalho",
  "license": "ISC",
  "devDependencies": {
    "@sentry/types": "^6.1.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^14.14.22",
    "@types/sharp": "^0.29.2",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "chalk": "4.1.2",
    "eslint": "^7.23.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.7",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@mypharma/api-core": "^0.18.43",
