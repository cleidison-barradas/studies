      "@babel/preset-typescript": "^7.12.7",
      "@sentry/types": "^6.1.0",
      "@types/amqplib": "^0.5.17",
      "@types/node": "^14.14.22",
      "nodemon": "^2.0.7",
      "typescript": "^4.1.3"
   },
   "dependencies": {
      "@sentry/node": "^6.1.0",
      "amqplib": "^0.6.0",
