{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "jsx-key": true,
        "jsx-no-string-ref": true,
        "jsx-space-before-trailing-slash": true,
        "jsx-boolean-value": false,
        "max-classes-per-file": false,
        "no-string-literal": false,
        "no-console": false,
        "semicolon": [
            true,
            "never"
        ],
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}