{
  "name": "@mypharma/api-core",
  "version": "0.18.50",
  "description": "The core for our API",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "node -r ts-node/register src/app.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build": "npm run lint && cti entrypoint -o index.ts ./src && tsc && cp package.json .npmrc build/",
    "deploy": "npm run build && cd build && npm publish",
    "deploy:release": "npm version minor --no-git-tag-version && npm run deploy",
    "deploy:patch": "npm version patch --no-git-tag-version --preid=patch && npm run deploy --tag=patch",
    "deploy:prerelease": "npm version prerelease --no-git-tag-version --preid=prerelease && npm run deploy --tag=prerelease"
  },
  "author": "George Carvalho",
  "license": "ISC",
  "devDependencies": {
    "@sentry/types": "^6.1.0",
    "@types/body-parser": "^1.19.0",
    "@types/bson": "^4.2.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.22",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "create-ts-index": "^1.13.6",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.23.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@sentry/node": "^6.0.4",
    "amqplib": "^0.7.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.12.1",
    "cors": "^2.8.5",
    "date-fns": "^2.21.3",
    "express": "^4.17.1",
    "ioredis": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^3.6.4",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typeorm": "github:LBRGeorge/typeorm",
    "validatorjs": "^3.22.1"
  }
}